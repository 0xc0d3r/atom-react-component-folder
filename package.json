{
  "name": "react-component-folder",
  "main": "./lib/react-component-folder",
  "version": "0.0.0",
  "description": "A short description of your package",
  "keywords": [],
  "repository": "https://github.com/atom/react-component-folder",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {},
  "configSchema": {
    "index.js": {
      "type": "boolean",
      "default": true
    },
    "styledComponents.js": {
      "type": "boolean",
      "default": true
    },
    "styles.js": {
      "type": "boolean",
      "default": true
    },
    "styles.less": {
      "type": "boolean",
      "default": true
    },
    "styles.scss": {
      "type": "boolean",
      "default": true
    },
    "index.story.js": {
      "type": "boolean",
      "default": true
    },
    "index.test.js": {
      "type": "boolean",
      "default": true
    },
    "index.jsTemplate": {
      "description": "Default template of index.js",
      "type": "string",
      "default":
        "/**\n * @flow\n * @author: Anesh Parvatha\n */\n\nimport React, { PureComponent } from 'react'\nimport { View } from 'react-native'\n\nimport styles from './styles'\n\ntype Props = {}\n\nclass $1 extends PureComponent<Props> {\n\trender() {\n\t\treturn (\n\t\t\t<View></View>\n\t\t)\n\t}\n}\n\nexport default $1"
    }
  }
}
